                "# Visualization of RQ2 Part B: Coaching Quality\n",
                "\n",
                "if 'llm_judge_df' in locals() and not llm_judge_df.empty:\n",
                "    # Set style\n",
                "    sns.set_style(\"whitegrid\")\n",
                "    plt.rcParams.update({'font.size': 12})\n",
                "\n",
                "    # 1. Mean Scores Bar Chart\n",
                "    metrics = ['Safety', 'Empathy', 'Actionability', 'Relevance', 'Overall Score']\n",
                "    mean_scores = llm_judge_df[metrics].mean()\n",
                "    \n",
                "    plt.figure(figsize=(10, 6))\n",
                "    ax = sns.barplot(x=mean_scores.index, y=mean_scores.values, palette=\"viridis\")\n",
                "    plt.title('RQ2 Part B: Average Coaching Quality Scores (LLM Judge)', fontsize=14, pad=20)\n",
                "    plt.ylabel('Mean Score (1-5)')\n",
                "    plt.ylim(0, 5.5)\n",
                "    \n",
                "    # Add value labels\n",
                "    for i, v in enumerate(mean_scores.values):\n",
                "        ax.text(i, v + 0.1, f\"{v:.2f}\", ha='center', va='bottom', fontweight='bold')\n",
                "        \n",
                "    # Add Thesis Target Line (e.g., > 4.0)\n",
                "    plt.axhline(y=4.0, color='r', linestyle='--', label='Thesis Target (>4.0)')\n",
                "    plt.legend()\n",
                "    plt.show()\n",
                "\n",
                "    # 2. Boxplot for Variance Analysis\n",
                "    plt.figure(figsize=(12, 6))\n",
                "    # Melt the dataframe for seaborn boxplot\n",
                "    melted_df = llm_judge_df[metrics].melt(var_name='Dimension', value_name='Score')\n",
                "    \n",
                "    sns.boxplot(x='Dimension', y='Score', data=melted_df, palette=\"Set2\")\n",
                "    sns.stripplot(x='Dimension', y='Score', data=melted_df, color=\".25\", alpha=0.5, jitter=True)\n",
                "    \n",
                "    plt.title('Distribution of Quality Scores across Scenarios', fontsize=14, pad=20)\n",
                "    plt.ylabel('Score (1-5)')\n",
                "    plt.ylim(0, 5.5)\n",
                "    plt.show()\n",
                "    \n",
                "    # 3. Correlation Heatmap\n",
                "    plt.figure(figsize=(8, 6))\n",
                "    sns.heatmap(llm_judge_df[metrics].corr(), annot=True, cmap='coolwarm', vmin=-1, vmax=1, fmt=\".2f\")\n",
                "    plt.title('Correlation between Quality Dimensions')\n",
                "    plt.show()\n",
                "\n",
                "    print(\"Visualization Complete.\")\n",
                "    print(f\"Total Scenarios Evaluated: {len(llm_judge_df)}\")\n",
                "else:\n",
                "    print(\"No data available for visualization. Please run the evaluation cell above.\")"

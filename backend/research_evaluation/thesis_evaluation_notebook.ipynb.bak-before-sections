                "elif _run_id_env:\n",
                "    RUN_DIR = ensure_dir(RUNS_DIR / _run_id_env)\n",
                "elif _resume_latest:\n",
                "    existing = sorted([p for p in RUNS_DIR.iterdir() if p.is_dir()], key=lambda p: p.name)\n",
                "    if existing:\n",
                "        RUN_DIR = ensure_dir(existing[-1])\n",
                "    else:\n",
                "        RUN_DIR = ensure_dir(RUNS_DIR / utc_now_iso().replace(\":\", \"-\").replace(\"+\", \"_\"))\n",
                "else:\n",

            "source": [
                "# Run RQ1.2 Evaluation (resumable)\n",
                "from pathlib import Path\n",
                "import json\n",
                "\n",
                "results_path = Path(RUN_DIR) / \"rq1_2_sta_results.csv\"\n",
                "checkpoint_path = Path(RUN_DIR) / \"rq1_2_sta_checkpoint.json\"\n",
                "\n",
                "results: list[dict] = []\n",
                "completed_ids: set[str] = set()\n",
                "\n",
                "if results_path.exists():\n",
                "    try:\n",
                "        prev_df = pd.read_csv(results_path)\n",
                "        if \"id\" in prev_df.columns:\n",
                "            completed_ids = {str(v) for v in prev_df[\"id\"].dropna().astype(str).unique().tolist()}\n",
                "            results = prev_df.to_dict(orient=\"records\")\n",
                "    except Exception:\n",
                "        completed_ids = set()\n",
                "        results = []\n",
                "\n",
                "if completed_ids:\n",
                "    print(f\"Resume: found {len(completed_ids)} completed scenarios; will skip them.\")\n",
                "\n",
                "print(\"Running RQ1.2: Retrospective Audit Evaluation (STA)...\")\n",
                "total = len(rq1_df)\n",
                "for iter_idx, (_, row) in enumerate(rq1_df.iterrows(), start=1):\n",
                "    scenario_id = str(row['id'])\n",
                "    if scenario_id in completed_ids:\n",
                "        continue\n",
                "\n",
                "    scenario = {\n",
                "        \"id\": row['id'],\n",
                "        \"turns\": row['turns'],\n",
                "        \"is_crisis\": row['is_crisis'],\n",
                "    }\n",
                "    ground_truth = bool(row['is_crisis'])\n",
                "\n",
                "    response, latency = evaluate_sta_audit(scenario)\n",
                "\n",
                "    if \"error\" in response:\n",
                "        predicted = None\n",
                "        risk_level = None\n",
                "        risk_trend = None\n",
                "        print(f\"API Error for scenario {row['id']}: {response['error']}\")\n",
                "    else:\n",
                "        risk_str = response.get('overall_risk_level')\n",
                "        risk_map = {'low': 0, 'moderate': 1, 'high': 2, 'critical': 3}\n",
                "        if isinstance(risk_str, str):\n",
                "            risk_level = risk_map.get(risk_str, -1)\n",
                "        else:\n",
                "            risk_level = -1\n",
                "        risk_trend = response.get('risk_trend')\n",
                "        predicted = (risk_level >= STA_CRISIS_THRESHOLD) if risk_level != -1 else None\n",
                "\n",
                "    row_out = {\n",
                "        \"id\": row['id'],\n",
                "        \"ground_truth\": ground_truth,\n",
                "        \"predicted\": predicted,\n",
                "        \"risk_level_str\": response.get('overall_risk_level'),\n",
                "        \"risk_level\": risk_level,\n",
                "        \"risk_trend\": risk_trend,\n",
                "        \"latency\": latency,\n",
                "        \"is_correct\": (ground_truth == predicted) if predicted is not None else None,\n",
                "    }\n",
                "    results.append(row_out)\n",
                "    completed_ids.add(scenario_id)\n",
                "\n",
                "    pd.DataFrame([row_out]).to_csv(\n",
                "        results_path,\n",
                "        mode=\"a\",\n",
                "        header=not results_path.exists(),\n",
                "        index=False,\n",
                "    )\n",
                "    checkpoint_path.write_text(\n",
                "        json.dumps({\"completed_ids\": sorted(completed_ids), \"last_id\": scenario_id}, indent=2),\n",
                "        encoding=\"utf-8\",\n",
                "    )\n",
                "\n",
                "    # Per-iteration visibility (requested): show the outcome for this scenario immediately.\n",
                "    report_iteration_result(\n",
                "        rq=\"RQ1.2\",\n",
                "        iteration=iter_idx,\n",
                "        total=total,\n",
                "        item_id=str(row['id']),\n",
                "        expected=bool(ground_truth),\n",
                "        predicted=predicted,\n",
                "        risk=str(response.get('overall_risk_level')),\n",
                "        latency_s=float(latency) if latency is not None else None,\n",
                "        error=str(response.get('error')) if isinstance(response, dict) and response.get('error') else None,\n",
                "    )\n",
                "\n",
                "if results_path.exists():\n",
                "    results_df = pd.read_csv(results_path)\n",
                "else:\n",
                "    results_df = pd.DataFrame(results)\n",
                "print(\"STA evaluation complete (or resumed).\")\n",
                "display(results_df)"
            ]
        },

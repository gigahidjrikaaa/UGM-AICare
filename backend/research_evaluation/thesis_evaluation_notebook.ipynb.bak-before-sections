            "source": [
                "# --- Disk-first rehydration (RQ1‚ÄìRQ3) ---\n",
                "\n",
                "# If your kernel resets, re-run the setup cells above,\n",
                "\n",
                "# then run this cell to restore common DataFrames from RUN_DIR.\n",
                "\n",
                "\n",
                "\n",
                "from __future__ import annotations\n",
                "\n",
                "\n",
                "\n",
                "from pathlib import Path\n",
                "\n",
                "import json\n",
                "\n",
                "import pandas as pd\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "def _try_read_csv(path: Path) -> pd.DataFrame:\n",
                "\n",
                "    if not path.exists():\n",
                "\n",
                "        return pd.DataFrame()\n",
                "\n",
                "    try:\n",
                "\n",
                "        return pd.read_csv(path)\n",
                "\n",
                "    except Exception as exc:\n",
                "\n",
                "        print(f\"Warning: failed to read CSV {path}: {exc}\")\n",
                "\n",
                "        return pd.DataFrame()\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "def rehydrate_run_artifacts(run_dir: Path | None = None) -> dict[str, object]:\n",
                "\n",
                "    \"\"\"Reload common notebook variables from persisted RUN_DIR artifacts.\n",
                "\n",
                "\n",
                "\n",
                "    This is a best-effort helper intended to reduce dependence on in-memory state.\n",
                "\n",
                "    It does not run the evaluations; it only reconstructs DataFrames/results for\n",
                "\n",
                "    downstream analysis/visualization cells.\n",
                "\n",
                "    \"\"\"\n",
                "\n",
                "\n",
                "\n",
                "    global aika_results\n",
                "\n",
                "    global results_df\n",
                "\n",
                "    global valid_results_df\n",
                "\n",
                "    global orchestration_results_df\n",
                "\n",
                "    global failures\n",
                "\n",
                "    global llm_judge_df\n",
                "\n",
                "    global rq3_privacy_metrics\n",
                "\n",
                "\n",
                "\n",
                "    run_dir = Path(run_dir or RUN_DIR)\n",
                "\n",
                "\n",
                "\n",
                "    # ---------------- RQ1.1 (Aika triage) ----------------\n",
                "\n",
                "    rq1_triage_path = run_dir / \"rq1_triage_results.csv\"\n",
                "\n",
                "    rq1_triage_df = _try_read_csv(rq1_triage_path)\n",
                "\n",
                "    if not rq1_triage_df.empty:\n",
                "\n",
                "        aika_results = rq1_triage_df.to_dict(orient=\"records\")\n",
                "\n",
                "        print(f\"Rehydrated RQ1.1: {len(aika_results)} rows from {rq1_triage_path}\")\n",
                "\n",
                "    else:\n",
                "\n",
                "        # Keep a predictable type for downstream cells.\n",
                "\n",
                "        aika_results = []\n",
                "\n",
                "        print(f\"RQ1.1: no results found at {rq1_triage_path}\")\n",
                "\n",
                "\n",
                "\n",
                "    # ---------------- RQ1.2 (STA audit) ----------------\n",
                "\n",
                "    rq1_2_path = run_dir / \"rq1_2_sta_results.csv\"\n",
                "\n",
                "    results_df = _try_read_csv(rq1_2_path)\n",
                "\n",
                "    if not results_df.empty and \"predicted\" in results_df.columns:\n",
                "\n",
                "        valid_results_df = results_df.dropna(subset=[\"predicted\"]).copy()\n",
                "\n",
                "        print(f\"Rehydrated RQ1.2: {len(results_df)} rows from {rq1_2_path}\")\n",
                "\n",
                "    else:\n",
                "\n",
                "        valid_results_df = pd.DataFrame()\n",
                "\n",
                "        print(f\"RQ1.2: no usable results found at {rq1_2_path}\")\n",
                "\n",
                "\n",
                "\n",
                "    # ---------------- RQ2-A (Orchestration) ----------------\n",
                "\n",
                "    rq2_path = run_dir / \"rq2_orchestration_results.csv\"\n",
                "\n",
                "    orchestration_results_df = _try_read_csv(rq2_path)\n",
                "\n",
                "    if not orchestration_results_df.empty:\n",
                "\n",
                "        if (\n",
                "\n",
                "            \"is_correct\" not in orchestration_results_df.columns\n",
                "\n",
                "            and {\"intent_match\", \"risk_match\", \"routing_match\"}.issubset(orchestration_results_df.columns)\n",
                "\n",
                "        ):\n",
                "\n",
                "            orchestration_results_df[\"is_correct\"] = (\n",
                "\n",
                "                orchestration_results_df[\"intent_match\"].astype(bool)\n",
                "\n",
                "                & orchestration_results_df[\"risk_match\"].astype(bool)\n",
                "\n",
                "                & orchestration_results_df[\"routing_match\"].astype(bool)\n",
                "\n",
                "            )\n",
                "\n",
                "        if \"is_correct\" in orchestration_results_df.columns:\n",
                "\n",
                "            failures = orchestration_results_df[orchestration_results_df[\"is_correct\"] == False]\n",
                "\n",
                "        else:\n",
                "\n",
                "            failures = pd.DataFrame()\n",
                "\n",
                "        print(f\"Rehydrated RQ2-A: {len(orchestration_results_df)} rows from {rq2_path}\")\n",
                "\n",
                "    else:\n",
                "\n",
                "        failures = pd.DataFrame()\n",
                "\n",
                "        print(f\"RQ2-A: no results found at {rq2_path}\")\n",
                "\n",
                "\n",
                "\n",
                "    # ---------------- RQ2-B (LLM Judge) ----------------\n",
                "\n",
                "    llm_judge_df = pd.DataFrame()\n",
                "\n",
                "    for candidate in (run_dir / \"rq2b_judge_results.csv\", run_dir / \"thesis_evaluation_results_rq3.csv\"):\n",
                "\n",
                "        df = _try_read_csv(candidate)\n",
                "\n",
                "        if not df.empty:\n",
                "\n",
                "            llm_judge_df = df\n",
                "\n",
                "            print(f\"Rehydrated RQ2-B judge: {len(llm_judge_df)} rows from {candidate}\")\n",
                "\n",
                "            break\n",
                "\n",
                "    if llm_judge_df.empty:\n",
                "\n",
                "        print(\"RQ2-B: no judge results found.\")\n",
                "\n",
                "\n",
                "\n",
                "    # ---------------- RQ3 (Privacy / k-anonymity) ----------------\n",
                "\n",
                "    rq3_privacy_metrics = {}\n",
                "\n",
                "    rq3_privacy_path = run_dir / \"rq3_privacy_metrics.json\"\n",
                "\n",
                "    if rq3_privacy_path.exists():\n",
                "\n",
                "        try:\n",
                "\n",
                "            rq3_privacy_metrics = json.loads(rq3_privacy_path.read_text(encoding=\"utf-8\"))\n",
                "\n",
                "            print(f\"Rehydrated RQ3 privacy metrics from {rq3_privacy_path}\")\n",
                "\n",
                "        except Exception as exc:\n",
                "\n",
                "            print(f\"Warning: failed to read {rq3_privacy_path}: {exc}\")\n",
                "\n",
                "\n",
                "\n",
                "    return {\n",
                "\n",
                "        \"aika_results\": aika_results,\n",
                "\n",
                "        \"results_df\": results_df,\n",
                "\n",
                "        \"valid_results_df\": valid_results_df,\n",
                "\n",
                "        \"orchestration_results_df\": orchestration_results_df,\n",
                "\n",
                "        \"failures\": failures,\n",
                "\n",
                "        \"llm_judge_df\": llm_judge_df,\n",
                "\n",
                "        \"rq3_privacy_metrics\": rq3_privacy_metrics,\n",
                "\n",
                "    }\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "_ = rehydrate_run_artifacts()\n"
            ]
        },

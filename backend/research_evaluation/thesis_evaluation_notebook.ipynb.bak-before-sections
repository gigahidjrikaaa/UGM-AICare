                "TIMEOUT = 300\n",
                "LANGFUSE_ENABLED = False\n",
                "\n",
                "# Reproducibility: write a small run manifest to runs/\n",
                "EVAL_ROOT = Path.cwd()\n",
                "RUNS_DIR = ensure_dir(EVAL_ROOT / \"runs\")\n",
                "\n",
                "# --- Run directory persistence (survives kernel reset) ---\n",
                "# Use ONE of the following, from most explicit to least:\n",
                "# 1) EVAL_RUN_DIR: absolute/relative path to an existing run folder\n",
                "# 2) EVAL_RUN_ID: folder name under runs/ (stable across restarts)\n",
                "# 3) EVAL_RESUME_LATEST=1: pick the most recent existing folder under runs/\n",
                "# 4) default: create a new timestamped run folder\n",
                "\n",
                "_run_dir_env = os.getenv(\"EVAL_RUN_DIR\")\n",
                "_run_id_env = os.getenv(\"EVAL_RUN_ID\")\n",
                "_resume_latest = os.getenv(\"EVAL_RESUME_LATEST\", \"\").strip().lower() in {\"1\", \"true\", \"yes\"}\n",
                "\n",
                "if _run_dir_env:\n",

        {
            "cell_type": "code",
            "execution_count": null,
            "id": "4d8ad5aa",
            "metadata": {},
            "outputs": [],
            "source": [
                "# --- Consolidate run artifacts into metrics.json + thesis-grade plots (CI + distributions) ---\n",
                "from pathlib import Path\n",
                "import json\n",
                "import math\n",
                "import re\n",
                "import pandas as pd\n",
                "import matplotlib.pyplot as plt\n",
                "import seaborn as sns\n",
                "\n",
                "from eval_lib.metrics_run import compute_and_write_metrics_for_run_dir\n",
                "\n",
                "\n",
                "def _wilson_ci(successes: int, n: int, z: float = 1.96) -> tuple[float, float]:\n",
                "    if n <= 0:\n",
                "        return (float('nan'), float('nan'))\n",
                "    p = successes / n\n",
                "    denom = 1 + (z**2) / n\n",
                "    center = (p + (z**2) / (2 * n)) / denom\n",
                "    half = (z / denom) * math.sqrt((p * (1 - p) / n) + (z**2) / (4 * n**2))\n",
                "    return (max(0.0, center - half), min(1.0, center + half))\n",
                "\n",
                "\n",
                "def _read_json(path: Path) -> dict:\n",
                "    with path.open(\"r\", encoding=\"utf-8\") as f:\n",
                "        return json.load(f)\n",
                "\n",
                "\n",
                "def _maybe_read_csv(path: Path) -> pd.DataFrame | None:\n",
                "    if not path.exists():\n",
                "        return None\n",
                "    try:\n",
                "        return pd.read_csv(path)\n",
                "    except Exception:\n",
                "        return None\n",
                "\n",
                "\n",
                "def _slug(value: str) -> str:\n",
                "    value = value.strip().lower()\n",
                "    value = re.sub(r\"[^a-z0-9]+\", \"_\", value)\n",
                "    value = re.sub(r\"_+\", \"_\", value).strip(\"_\")\n",
                "    return value or \"run\"\n",
                "\n",
                "\n",
                "def _export_fig(fig: plt.Figure, base_name: str, export_dir: Path, run_tag: str) -> Path:\n",
                "    export_dir.mkdir(parents=True, exist_ok=True)\n",
                "    filename = f\"{run_tag}__{base_name}.png\"\n",
                "    out = export_dir / filename\n",
                "    fig.savefig(out, dpi=200, bbox_inches=\"tight\")\n",
                "    return out\n",
                "\n",
                "\n",
                "RUN_DIR = Path(RUN_DIR)  # ensure Path\n",
                "EXPORT_DIR = Path.cwd() / \"results\"\n",
                "RUN_TAG = _slug(RUN_DIR.name)\n",
                "metrics_path = compute_and_write_metrics_for_run_dir(RUN_DIR)\n",
                "metrics = _read_json(metrics_path)\n",
                "print(f\"Loaded metrics from: {metrics_path}\")\n",
                "print(f\"Exporting figures to: {EXPORT_DIR}\")\n",
                "\n",
                "# ------------------ RQ1: CIs + latency distribution ------------------\n",
                "rq1_csv = None\n",
                "for name in (\"rq1_triage_results.csv\", \"rq1_results.csv\"):\n",
                "    candidate = RUN_DIR / name\n",
                "    if candidate.exists():\n",
                "        rq1_csv = candidate\n",
                "        break\n",
                "\n",
                "if rq1_csv is not None:\n",
                "    df1 = pd.read_csv(rq1_csv)\n",
                "    # Expect columns: expected_is_crisis, predicted_is_crisis, latency_ms (best-effort)\n",
                "    expected_col = next((c for c in df1.columns if c.lower() in (\"expected_is_crisis\", \"expected\", \"gold_is_crisis\", \"label\")), None)\n",
                "    pred_col = next((c for c in df1.columns if c.lower() in (\"predicted_is_crisis\", \"predicted\", \"prediction\", \"is_crisis_pred\")), None)\n",
                "    latency_col = next((c for c in df1.columns if \"latency\" in c.lower()), None)\n",
                "    if expected_col and pred_col:\n",
                "        y_true = df1[expected_col].astype(bool)\n",
                "        y_pred = df1[pred_col].astype(bool)\n",
                "        tp = int(((y_true == True) & (y_pred == True)).sum())\n",
                "        tn = int(((y_true == False) & (y_pred == False)).sum())\n",
                "        fp = int(((y_true == False) & (y_pred == True)).sum())\n",
                "        fn = int(((y_true == True) & (y_pred == False)).sum())\n",
                "\n",
                "        sens_n = tp + fn\n",
                "        spec_n = tn + fp\n",
                "        prec_n = tp + fp\n",
                "        sens = tp / sens_n if sens_n else float('nan')\n",
                "        spec = tn / spec_n if spec_n else float('nan')\n",
                "        prec = tp / prec_n if prec_n else float('nan')\n",
                "        sens_ci = _wilson_ci(tp, sens_n)\n",
                "        spec_ci = _wilson_ci(tn, spec_n)\n",
                "        prec_ci = _wilson_ci(tp, prec_n)\n",
                "\n",
                "        plt.figure(figsize=(7, 4))\n",
                "        labels = [\"Sensitivity\", \"Specificity\", \"Precision\"]\n",
                "        vals = [sens, spec, prec]\n",
                "        lows = [sens - sens_ci[0] if not math.isnan(sens) else 0, spec - spec_ci[0] if not math.isnan(spec) else 0, prec - prec_ci[0] if not math.isnan(prec) else 0]\n",
                "        highs = [sens_ci[1] - sens if not math.isnan(sens) else 0, spec_ci[1] - spec if not math.isnan(spec) else 0, prec_ci[1] - prec if not math.isnan(prec) else 0]\n",
                "        plt.errorbar(x=labels, y=vals, yerr=[lows, highs], fmt='o', capsize=5)\n",
                "        plt.ylim(0, 1)\n",
                "        plt.title(\"RQ1: Proportion Metrics with 95% Wilson CI\")\n",
                "        plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "        fig = plt.gcf()\n",
                "        out = _export_fig(fig, \"rq1_ci_proportions\", EXPORT_DIR, RUN_TAG)\n",
                "        print(f\"Wrote: {out}\")\n",
                "        plt.show()\n",
                "\n",
                "    if latency_col:\n",
                "        lat = pd.to_numeric(df1[latency_col], errors='coerce').dropna()\n",
                "        if not lat.empty:\n",
                "            p50 = float(lat.quantile(0.50))\n",
                "            p95 = float(lat.quantile(0.95))\n",
                "            plt.figure(figsize=(8, 4))\n",
                "            sns.histplot(lat, bins=20, kde=True)\n",
                "            plt.axvline(p50, color='k', linestyle='--', label=f\"p50={p50:.0f} ms\")\n",
                "            plt.axvline(p95, color='r', linestyle='--', label=f\"p95={p95:.0f} ms\")\n",
                "            plt.title(\"RQ1: Latency Distribution\")\n",
                "            plt.xlabel(\"Latency (ms)\")\n",
                "            plt.legend()\n",
                "            plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "            fig = plt.gcf()\n",
                "            out = _export_fig(fig, \"rq1_latency_distribution\", EXPORT_DIR, RUN_TAG)\n",
                "            print(f\"Wrote: {out}\")\n",
                "            plt.show()\n",
                "\n",
                "# ------------------ RQ2: Flow-level pass + CI; per-field CIs ------------------\n",
                "rq2_csv = None\n",
                "for name in (\"rq2_orchestration_results.csv\", \"rq2_results.csv\"):\n",
                "    candidate = RUN_DIR / name\n",
                "    if candidate.exists():\n",
                "        rq2_csv = candidate\n",
                "        break\n",
                "\n",
                "if rq2_csv is not None:\n",
                "    df2 = pd.read_csv(rq2_csv)\n",
                "    # Try to infer core columns\n",
                "    flow_col = next((c for c in df2.columns if c.lower() in (\"flow_id\", \"flow\", \"scenario_id\")), None)\n",
                "    intent_ok = next((c for c in df2.columns if c.lower() in (\"intent_correct\", \"is_intent_correct\")), None)\n",
                "    risk_ok = next((c for c in df2.columns if c.lower() in (\"risk_correct\", \"is_risk_correct\")), None)\n",
                "    agent_ok = next((c for c in df2.columns if c.lower() in (\"agent_correct\", \"is_agent_correct\", \"next_agent_correct\")), None)\n",
                "\n",
                "    if intent_ok or risk_ok or agent_ok:\n",
                "        # Turn-level CIs\n",
                "        plt.figure(figsize=(7, 4))\n",
                "        items = []\n",
                "        for label, col in [(\"Intent\", intent_ok), (\"Risk\", risk_ok), (\"Next Agent\", agent_ok)]:\n",
                "            if not col:\n",
                "                continue\n",
                "            series = df2[col].astype(bool)\n",
                "            n = int(series.notna().sum())\n",
                "            s = int(series.sum())\n",
                "            p = s / n if n else float('nan')\n",
                "            lo, hi = _wilson_ci(s, n)\n",
                "            items.append((label, p, p - lo, hi - p))\n",
                "        if items:\n",
                "            labels = [x[0] for x in items]\n",
                "            vals = [x[1] for x in items]\n",
                "            lows = [x[2] for x in items]\n",
                "            highs = [x[3] for x in items]\n",
                "            plt.errorbar(x=labels, y=vals, yerr=[lows, highs], fmt='o', capsize=5)\n",
                "            plt.ylim(0, 1)\n",
                "            plt.title(\"RQ2: Turn-level Accuracy with 95% Wilson CI\")\n",
                "            plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "            fig = plt.gcf()\n",
                "            out = _export_fig(fig, \"rq2_turn_accuracy_ci\", EXPORT_DIR, RUN_TAG)\n",
                "            print(f\"Wrote: {out}\")\n",
                "            plt.show()\n",
                "\n",
                "    # Flow-level pass: all turns correct across available fields\n",
                "    if flow_col and (intent_ok or risk_ok or agent_ok):\n",
                "        def row_pass(row) -> bool:\n",
                "            checks = []\n",
                "            for col in (intent_ok, risk_ok, agent_ok):\n",
                "                if col and col in row:\n",
                "                    checks.append(bool(row[col]))\n",
                "            return all(checks) if checks else False\n",
                "        df2[\"__turn_pass\"] = df2.apply(row_pass, axis=1)\n",
                "        flow_pass = df2.groupby(flow_col)[\"__turn_pass\"].all()\n",
                "        n_flows = int(flow_pass.shape[0])\n",
                "        s_flows = int(flow_pass.sum())\n",
                "        rate = s_flows / n_flows if n_flows else float('nan')\n",
                "        lo, hi = _wilson_ci(s_flows, n_flows)\n",
                "        plt.figure(figsize=(5, 4))\n",
                "        plt.bar([\"Flow pass\"], [rate], yerr=[[rate - lo], [hi - rate]], capsize=6)\n",
                "        plt.ylim(0, 1)\n",
                "        plt.title(\"RQ2: Flow-level Pass Rate (95% Wilson CI)\")\n",
                "        plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "        fig = plt.gcf()\n",
                "        out = _export_fig(fig, \"rq2_flow_pass_rate_ci\", EXPORT_DIR, RUN_TAG)\n",
                "        print(f\"Wrote: {out}\")\n",
                "        plt.show()\n",
                "\n",
                "# ------------------ RQ2B: Score distributions (box/violin) ------------------\n",
                "judge_candidates = [\n",
                "    RUN_DIR / \"rq2b_judge_results.csv\",\n",
                "    RUN_DIR / \"thesis_evaluation_results_rq3.csv\",\n",
                "    RUN_DIR / \"rq2b_judge.csv\",\n",
                "    RUN_DIR / \"judge_results.csv\",\n",
                "    # fallback: paths recorded by runner if present\n",
                "    Path(metrics.get(\"rq2b\", {}).get(\"source_judge\", \"\")) if metrics.get(\"rq2b\", {}).get(\"source_judge\") else None,\n",
                "    Path(metrics.get(\"rq2b\", {}).get(\"judge_csv\", \"\")) if metrics.get(\"rq2b\", {}).get(\"judge_csv\") else None,\n",
                " ]\n",
                "judge_df = None\n",
                "for cand in judge_candidates:\n",
                "    if cand is None:\n",
                "        continue\n",
                "    cand = Path(cand)\n",
                "    judge_df = _maybe_read_csv(cand)\n",
                "    if judge_df is not None:\n",
                "        print(f\"Loaded judge CSV: {cand}\")\n",
                "        break\n",
                "\n",
                "if judge_df is not None:\n",
                "    # Normalize common column names\n",
                "    col_map = {}\n",
                "    for c in judge_df.columns:\n",
                "        cl = c.strip().lower()\n",
                "        if cl in (\"safety\",):\n",
                "            col_map[c] = \"Safety\"\n",
                "        elif cl in (\"empathy\",):\n",
                "            col_map[c] = \"Empathy\"\n",
                "        elif cl in (\"actionability\",):\n",
                "            col_map[c] = \"Actionability\"\n",
                "        elif cl in (\"relevance\",):\n",
                "            col_map[c] = \"Relevance\"\n",
                "        elif cl in (\"overall\", \"overall_score\", \"overall score\"):\n",
                "            col_map[c] = \"Overall\"\n",
                "    judge_df = judge_df.rename(columns=col_map)\n",
                "    score_cols = [c for c in [\"Safety\", \"Empathy\", \"Actionability\", \"Relevance\", \"Overall\"] if c in judge_df.columns]\n",
                "    if score_cols:\n",
                "        melted = judge_df[score_cols].melt(var_name=\"Dimension\", value_name=\"Score\")\n",
                "        melted[\"Score\"] = pd.to_numeric(melted[\"Score\"], errors='coerce')\n",
                "        melted = melted.dropna()\n",
                "        plt.figure(figsize=(9, 4))\n",
                "        sns.violinplot(data=melted, x=\"Dimension\", y=\"Score\", inner=\"box\")\n",
                "        plt.title(\"RQ2B: Judge Score Distributions\")\n",
                "        plt.ylim(0, 5)\n",
                "        plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "        fig = plt.gcf()\n",
                "        out = _export_fig(fig, \"rq2b_judge_score_distributions\", EXPORT_DIR, RUN_TAG)\n",
                "        print(f\"Wrote: {out}\")\n",
                "        plt.show()\n",
                "\n",
                "# ------------------ Reliability: retries/failures from progress.jsonl ------------------\n",
                "progress_path = RUN_DIR / \"progress.jsonl\"\n",
                "if progress_path.exists():\n",
                "    events = []\n",
                "    with progress_path.open(\"r\", encoding=\"utf-8\") as f:\n",
                "        for line in f:\n",
                "            line = line.strip()\n",
                "            if not line:\n",
                "                continue\n",
                "            try:\n",
                "                events.append(json.loads(line))\n",
                "            except Exception:\n",
                "                continue\n",
                "    if events:\n",
                "        dfp = pd.DataFrame(events)\n",
                "        # attempt counts typically stored on completion events; best-effort extraction\n",
                "        attempt_col = next((c for c in dfp.columns if c.lower() in (\"attempt\", \"attempts_used\", \"attempt_count\")), None)\n",
                "        status_col = next((c for c in dfp.columns if c.lower() in (\"status\", \"event\", \"type\")), None)\n",
                "        if attempt_col:\n",
                "            attempts = pd.to_numeric(dfp[attempt_col], errors='coerce').dropna()\n",
                "            if not attempts.empty:\n",
                "                plt.figure(figsize=(7, 4))\n",
                "                sns.countplot(x=attempts.astype(int))\n",
                "                plt.title(\"Reliability: Attempts Used Distribution\")\n",
                "                plt.xlabel(\"Attempts used\")\n",
                "                plt.ylabel(\"Count\")\n",
                "                plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "                fig = plt.gcf()\n",
                "                out = _export_fig(fig, \"reliability_attempts_used_distribution\", EXPORT_DIR, RUN_TAG)\n",
                "                print(f\"Wrote: {out}\")\n",
                "                plt.show()\n",
                "        if status_col:\n",
                "            plt.figure(figsize=(7, 3))\n",
                "            dfp[status_col].astype(str).value_counts().head(10).plot(kind=\"bar\")\n",
                "            plt.title(\"Reliability: Top Progress Event Types\")\n",
                "            plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
                "            fig = plt.gcf()\n",
                "            out = _export_fig(fig, \"reliability_progress_event_types\", EXPORT_DIR, RUN_TAG)\n",
                "            print(f\"Wrote: {out}\")\n",
                "            plt.show()"
            ]
        },

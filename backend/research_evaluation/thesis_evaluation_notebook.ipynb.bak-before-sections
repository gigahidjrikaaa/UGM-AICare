            ],
            "source": [
                "# Run the orchestration evaluation for all flows (per-flow traceability + retries)\n",
                "from pathlib import Path\n",
                "import json\n",
                "from tqdm.auto import tqdm\n",
                "\n",
                "from eval_lib.retry_utils import NonRetriableError\n",
                "\n",
                "results_path = Path(RUN_DIR) / \"rq2_orchestration_results.csv\"\n",
                "failures_path = Path(RUN_DIR) / \"rq2_orchestration_failures.csv\"\n",
                "checkpoint_path = Path(RUN_DIR) / \"rq2_orchestration_checkpoint.json\"\n",
                "\n",
                "def _read_existing_flow_ids(path: Path) -> set[str]:\n",
                "    if not path.exists():\n",
                "        return set()\n",
                "    try:\n",
                "        df = pd.read_csv(path)\n",
                "    except Exception:\n",
                "        return set()\n",
                "    if \"flow_id\" not in df.columns:\n",
                "        return set()\n",
                "    return {str(v) for v in df[\"flow_id\"].dropna().astype(str).unique().tolist()}\n",
                "\n",
                "completed_flow_ids = set()\n",
                "completed_flow_ids |= _read_existing_flow_ids(results_path)\n",
                "completed_flow_ids |= _read_existing_flow_ids(failures_path)\n",
                "\n",
                "if completed_flow_ids:\n",
                "    print(f\"Resume: found {len(completed_flow_ids)} completed flows; will skip them.\")\n",
                "\n",
                "total = len(rq2_dataset)\n",
                "for iter_idx, flow in enumerate(tqdm(rq2_dataset, total=total), start=1):\n",
                "    flow_id = str(flow.get(\"flow_id\") or \"\")\n",
                "    if not flow_id:\n",
                "        continue\n",
                "    if flow_id in completed_flow_ids:\n",
                "        continue\n",
                "\n",
                "    test_id = f\"rq2_orchestration::{flow_id}\"\n",
                "    try:\n",
                "        result = run_with_retry(test_id, lambda: evaluate_orchestration(flow))\n",
                "    except NonRetriableError as exc:\n",
                "        # Persist checkpoint and stop this RQ cell so it can be resumed later.\n",
                "        payload = {\n",
                "            \"stopped_reason\": \"NON_RETRIABLE\",\n",
                "            \"message\": str(exc),\n",
                "            \"completed_flows\": sorted(completed_flow_ids),\n",
                "            \"last_flow_id\": flow_id,\n",
                "        }\n",
                "        checkpoint_path.write_text(json.dumps(payload, indent=2), encoding=\"utf-8\")\n",
                "        print(f\"\\n‚è∏Ô∏è Stopping RQ2-A due to non-retriable error: {exc}\")\n",
                "        print(f\"Wrote: {checkpoint_path}\")\n",
                "        break\n",
                "\n",
                "    if isinstance(result, dict) and result.get(\"error\"):\n",
                "        failure_row = {\n",
                "            \"flow_id\": flow_id,\n",
                "            \"error\": result.get(\"error\"),\n",
                "            \"attempts_used\": result.get(\"attempts_used\"),\n",
                "        }\n",
                "        pd.DataFrame([failure_row]).to_csv(\n",
                "            failures_path,\n",
                "            mode=\"a\",\n",
                "            header=not failures_path.exists(),\n",
                "            index=False,\n",
                "        )\n",
                "\n",
                "        # Record a single synthetic failure row so downstream metrics still work\n",
                "        synthetic = {\n",
                "            \"flow_id\": flow_id,\n",
                "            \"turn_index\": None,\n",
                "            \"user_input\": None,\n",
                "            \"expected_intent\": None,\n",
                "            \"actual_intent\": \"ERROR\",\n",
                "            \"expected_risk\": None,\n",
                "            \"actual_risk\": \"ERROR\",\n",
                "            \"expected_next_agent\": None,\n",
                "            \"actual_next_agent\": \"ERROR\",\n",
                "            \"intent_match\": False,\n",
                "            \"risk_match\": False,\n",
                "            \"routing_match\": False,\n",
                "            \"error\": result.get(\"error\"),\n",
                "            \"attempts_used\": result.get(\"attempts_used\"),\n",
                "        }\n",
                "        pd.DataFrame([synthetic]).to_csv(\n",
                "            results_path,\n",
                "            mode=\"a\",\n",
                "            header=not results_path.exists(),\n",
                "            index=False,\n",
                "        )\n",
                "\n",
                "        completed_flow_ids.add(flow_id)\n",
                "        report_iteration_result(\n",
                "            rq=\"RQ2-A\",\n",
                "            iteration=iter_idx,\n",
                "            total=total,\n",
                "            item_id=str(flow_id),\n",
                "            expected=\"NO_ERROR\",\n",
                "            predicted=\"ERROR\",\n",
                "            error=str(result.get(\"error\")),\n",
                "            attempts_used=int(result.get(\"attempts_used\")) if isinstance(result.get(\"attempts_used\"), int) else None,\n",
                "        )\n",
                "        continue\n",
                "\n",
                "    # Normal success path: unpack (turn_results, execution_ids)\n",
                "    flow_results, flow_exec_ids = result\n",
                "    if flow_results:\n",
                "        pd.DataFrame(flow_results).to_csv(\n",
                "            results_path,\n",
                "            mode=\"a\",\n",
                "            header=not results_path.exists(),\n",
                "            index=False,\n",
                "        )\n",
                "\n",
                "    completed_flow_ids.add(flow_id)\n",
                "    checkpoint_path.write_text(\n",
                "        json.dumps({\"completed_flows\": sorted(completed_flow_ids), \"last_flow_id\": flow_id}, indent=2),\n",
                "        encoding=\"utf-8\",\n",
                "    )\n",
                "\n",
                "    # Per-iteration visibility (requested): summarize this flow immediately.\n",
                "    flow_total_turns = len(flow_results)\n",
                "    flow_correct_turns = sum(\n",
                "        1\n",
                "        for r in flow_results\n",
                "        if bool(r.get(\"intent_match\")) and bool(r.get(\"risk_match\")) and bool(r.get(\"routing_match\"))\n",
                "    )\n",
                "    flow_accuracy = (flow_correct_turns / flow_total_turns) if flow_total_turns > 0 else 0.0\n",
                "    report_iteration_result(\n",
                "        rq=\"RQ2-A\",\n",
                "        iteration=iter_idx,\n",
                "        total=total,\n",
                "        item_id=str(flow_id),\n",
                "        expected=flow_total_turns,\n",
                "        predicted=flow_correct_turns,\n",
                "        extra={\"flow_accuracy\": f\"{flow_accuracy:.2%}\"},\n",
                "    )\n",
                "\n",
                "# Load full results for display and metrics.\n",
                "if results_path.exists():\n",
                "    orchestration_results_df = pd.read_csv(results_path)\n",
                "else:\n",
                "    orchestration_results_df = pd.DataFrame([])\n",
                "\n",
                "print(\"Orchestration evaluation complete (or paused).\")\n",
                "if results_path.exists():\n",
                "    print(f\"Wrote: {results_path}\")\n",
                "if failures_path.exists():\n",
                "    print(f\"Wrote: {failures_path}\")\n",
                "\n",
                "# Calculate State Transition Accuracy\n",
                "if not orchestration_results_df.empty:\n",
                "    if \"is_correct\" not in orchestration_results_df.columns:\n",
                "        orchestration_results_df['is_correct'] = (\n",
                "            orchestration_results_df['intent_match'].astype(bool) &\n",
                "            orchestration_results_df['risk_match'].astype(bool) &\n",
                "            orchestration_results_df['routing_match'].astype(bool)\n",
                "        )\n",
                "    correct_transitions = orchestration_results_df['is_correct'].sum()\n",
                "    total_transitions = len(orchestration_results_df)\n",
                "    accuracy = (correct_transitions / total_transitions) if total_transitions > 0 else 0\n",
                "    print(f\"\\n--- State Transition Accuracy ---\")\n",
                "    print(f\"Accuracy: {accuracy:.2%}\")\n",
                "else:\n",
                "    print(\"Could not calculate accuracy due to empty results.\")\n",
                "\n",
                "display(orchestration_results_df)"
            ]
        },

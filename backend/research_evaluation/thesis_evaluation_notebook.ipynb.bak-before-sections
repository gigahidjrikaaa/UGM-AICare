                        "Results saved to 'thesis_evaluation_results_rq3.csv'\n"
                    ]
                },
                {
                    "name": "stderr",
                    "output_type": "stream",
                    "text": [
                        "\n"
                    ]
                }
            ],
            "source": [
                "# Run Evaluation on All Generated Responses (resumable)\n",
                "from pathlib import Path\n",
                "import csv\n",
                "import json\n",
                "\n",
                "judge_csv_path = Path(RUN_DIR) / \"rq2b_judge_results.csv\"\n",
                "judge_csv_legacy_path = Path(RUN_DIR) / \"thesis_evaluation_results_rq3.csv\"\n",
                "generated_path = Path(RUN_DIR) / \"rq2b_generated.json\"\n",
                "\n",
                "def _load_generated_responses_for_evaluation() -> list[dict]:\n",
                "    if not generated_path.exists():\n",
                "        return []\n",
                "    try:\n",
                "        rows = json.loads(generated_path.read_text(encoding=\"utf-8\"))\n",
                "    except Exception:\n",
                "        return []\n",
                "    if not isinstance(rows, list):\n",
                "        return []\n",
                "\n",
                "    out: list[dict] = []\n",
                "\n",
                "    def _format_plan_for_judge(response: dict) -> str:\n",
                "        def _coerce(item: dict | object, field: str):\n",
                "            if isinstance(item, dict):\n",
                "                return item.get(field)\n",
                "            return getattr(item, field, None)\n",
                "\n",
                "        plan_steps = response.get('plan_steps') or []\n",
                "        if plan_steps:\n",
                "            step_lines = []\n",
                "            for idx, step in enumerate(plan_steps, start=1):\n",
                "                label = _coerce(step, 'title') or \"(missing title)\"\n",
                "                desc = _coerce(step, 'description') or \"\"\n",
                "                duration = _coerce(step, 'duration_min')\n",
                "                if duration:\n",
                "                    step_lines.append(f\"{idx}. {label}: {desc} ({duration} min)\")\n",
                "                else:\n",
                "                    step_lines.append(f\"{idx}. {label}: {desc}\")\n",
                "            steps_block = \"\\n\".join(step_lines)\n",
                "        else:\n",
                "            steps_block = \"(No plan steps returned)\"\n",
                "\n",
                "        resource_cards = response.get('resource_cards') or []\n",
                "        if resource_cards:\n",
                "            resource_lines = []\n",
                "            for card in resource_cards:\n",
                "                title = _coerce(card, 'title') or \"Resource\"\n",
                "                summary = _coerce(card, 'description')\n",
                "                parts = [title]\n",
                "                if summary:\n",
                "                    parts.append(summary)\n",
                "                resource_lines.append(\" - \" + \" | \".join(parts))\n",
                "            resources_block = \"\\n\".join(resource_lines)\n",
                "        else:\n",
                "            resources_block = \"(No resource cards returned)\"\n",
                "\n",
                "        next_check_in = response.get('next_check_in') or 'N/A'\n",
                "        return (\n",
                "            f\"Plan Steps:\\n{steps_block}\\n\\n\"\n",
                "            f\"Resource Cards:\\n{resources_block}\\n\\n\"\n",
                "            f\"Next Check-in: {next_check_in}\"\n",
                "        )\n",
                "\n",
                "    for row in rows:\n",
                "        scenario_id = str(row.get(\"scenario_id\") or \"\")\n",
                "        if not scenario_id:\n",
                "            continue\n",
                "        prompt = row.get(\"prompt\")\n",
                "        category = row.get(\"category\")\n",
                "        resp = row.get(\"sca_response\")\n",
                "        error = row.get(\"error\")\n",
                "        if isinstance(resp, dict):\n",
                "            response_text = _format_plan_for_judge(resp)\n",
                "        elif error:\n",
                "            response_text = f\"API_ERROR: {error}\"\n",
                "        else:\n",
                "            response_text = \"API_ERROR: missing sca_response\"\n",
                "        out.append({\n",
                "            \"scenario_id\": scenario_id,\n",
                "            \"prompt\": prompt,\n",
                "            \"category\": category,\n",
                "            \"response_text\": response_text,\n",
                "        })\n",
                "    return out\n",
                "\n",
                "def _read_completed_judge_ids(path: Path) -> set[str]:\n",
                "    if not path.exists():\n",
                "        return set()\n",
                "    try:\n",
                "        df = pd.read_csv(path)\n",
                "    except Exception:\n",
                "        return set()\n",
                "    if \"Scenario ID\" not in df.columns:\n",
                "        return set()\n",
                "    return {str(v) for v in df[\"Scenario ID\"].dropna().astype(str).unique().tolist()}\n",
                "\n",
                "def _append_judge_row(path: Path, row: dict) -> None:\n",
                "    header = [\"Scenario ID\", \"Scenario\", \"Generated Plan\", \"Safety\", \"Empathy\", \"Actionability\", \"Relevance\", \"Overall Score\", \"Reasoning\"]\n",
                "    is_new = not path.exists()\n",
                "    with path.open(\"a\", newline=\"\", encoding=\"utf-8\") as f:\n",
                "        w = csv.DictWriter(f, fieldnames=header)\n",
                "        if is_new:\n",
                "            w.writeheader()\n",
                "        w.writerow({k: row.get(k) for k in header})\n",
                "\n",
                "llm_judge_results = []\n",
                "\n",
                "# Ensure we have responses to evaluate (kernel-reset safe)\n",
                "if 'responses_for_evaluation' not in locals() or not responses_for_evaluation:\n",
                "    responses_for_evaluation = _load_generated_responses_for_evaluation()\n",
                "    if responses_for_evaluation:\n",
                "        print(f\"Loaded {len(responses_for_evaluation)} responses from: {generated_path}\")\n",
                "\n",
                "if not responses_for_evaluation:\n",
                "    print(\"No responses found. Please run the generation cell above first.\")\n",
                "else:\n",
                "    completed_ids = _read_completed_judge_ids(judge_csv_path)\n",
                "    if completed_ids:\n",
                "        print(f\"Resume: found {len(completed_ids)} completed judge rows; will skip them.\")\n",
                "\n",
                "    pending = [r for r in responses_for_evaluation if str(r.get(\"scenario_id\")) not in completed_ids]\n",
                "    print(f\"Starting LLM-as-a-Judge evaluation ({OPENROUTER_MODEL})...\")\n",
                "    print(f\"Evaluating {len(pending)}/{len(responses_for_evaluation)} pending scenarios.\")\n",
                "\n",
                "    # Concurrency Control to avoid Rate Limits\n",
                "    CONCURRENCY_LIMIT = 5\n",
                "    semaphore = asyncio.Semaphore(CONCURRENCY_LIMIT)\n",
                "\n",
                "    async def evaluate_single_entry(entry, pbar):\n",
                "        async with semaphore:\n",
                "            scenario_id = entry['scenario_id']\n",
                "            response_text = entry['response_text']\n",
                "            scenario_text = entry['prompt']\n",
                "\n",
                "            max_retries = 10\n",
                "            timeout_seconds = 120\n",
                "\n",
                "            for attempt in range(max_retries):\n",
                "                try:\n",
                "                    if attempt > 0:\n",
                "                        pbar.write(f\"  Scenario {scenario_id}: Retry {attempt+1}/{max_retries}...\")\n",
                "\n",
                "                    eval_result = await asyncio.wait_for(\n",
                "                        evaluate_plan_with_openrouter(scenario_text, response_text),\n",
                "                        timeout=timeout_seconds,\n",
                "                    )\n",
                "                    if not eval_result:\n",
                "                        raise ValueError(\"Empty result from judge\")\n",
                "\n",
                "                    pbar.write(\n",
                "                        f\"  Scenario {scenario_id}: Safety={eval_result.get('safety_score')}, \"\n",
                "                        f\"Empathy={eval_result.get('empathy_score')}, \"\n",
                "                        f\"Action={eval_result.get('actionability_score')}, \"\n",
                "                        f\"Rel={eval_result.get('relevance_score')} \"\n",
                "                        f\"-> Overall={eval_result.get('overall_score')}\"\n",
                "                    )\n",
                "                    pbar.update(1)\n",
                "                    return {\n",
                "                        \"scenario_id\": scenario_id,\n",
                "                        \"scenario\": scenario_text,\n",
                "                        \"generated_plan\": response_text,\n",
                "                        \"evaluation\": eval_result,\n",
                "                    }\n",
                "                except asyncio.TimeoutError:\n",
                "                    pbar.write(f\"  Scenario {scenario_id}: ‚ö†Ô∏è Timeout after {timeout_seconds}s (Attempt {attempt+1})\")\n",
                "                except Exception as e:\n",
                "                    pbar.write(f\"  Scenario {scenario_id}: ‚ö†Ô∏è Error: {str(e)} (Attempt {attempt+1})\")\n",
                "\n",
                "                await asyncio.sleep(2 * (attempt + 1))\n",
                "\n",
                "            pbar.write(f\"  Scenario {scenario_id}: ‚ùå FAILED after {max_retries} attempts.\")\n",
                "            pbar.update(1)\n",
                "            return {\n",
                "                \"scenario_id\": scenario_id,\n",
                "                \"scenario\": scenario_text,\n",
                "                \"generated_plan\": response_text,\n",
                "                \"evaluation\": {\n",
                "                    \"safety_score\": 0,\n",
                "                    \"empathy_score\": 0,\n",
                "                    \"actionability_score\": 0,\n",
                "                    \"relevance_score\": 0,\n",
                "                    \"overall_score\": 0,\n",
                "                    \"reasoning\": \"Evaluation Failed: Timeout or API Error after multiple retries\",\n",
                "                },\n",
                "            }\n",
                "\n",
                "    if pending:\n",
                "        pbar = tqdm(total=len(pending), desc=\"Evaluating with Judge\")\n",
                "        tasks = [asyncio.create_task(evaluate_single_entry(entry, pbar)) for entry in pending]\n",
                "        for fut in asyncio.as_completed(tasks):\n",
                "            r = await fut\n",
                "            llm_judge_results.append(r)\n",
                "            row_out = {\n",
                "                \"Scenario ID\": r['scenario_id'],\n",
                "                \"Scenario\": r['scenario'],\n",
                "                \"Generated Plan\": r['generated_plan'],\n",
                "                \"Safety\": r['evaluation'].get('safety_score', 0),\n",
                "                \"Empathy\": r['evaluation'].get('empathy_score', 0),\n",
                "                \"Actionability\": r['evaluation'].get('actionability_score', 0),\n",
                "                \"Relevance\": r['evaluation'].get('relevance_score', 0),\n",
                "                \"Overall Score\": r['evaluation'].get('overall_score', 0),\n",
                "                \"Reasoning\": r['evaluation'].get('reasoning', ''),\n",
                "            }\n",
                "            _append_judge_row(judge_csv_path, row_out)\n",
                "        pbar.close()\n",
                "\n",
                "        print(\"\\nEvaluation complete.\")\n",
                "        print(f\"Wrote: {judge_csv_path}\")\n",
                "\n",
                "        # Keep a legacy copy name (inside RUN_DIR) for older downstream expectations.\n",
                "        try:\n",
                "            if judge_csv_path.exists():\n",
                "                pd.read_csv(judge_csv_path).to_csv(judge_csv_legacy_path, index=False)\n",
                "                print(f\"Wrote: {judge_csv_legacy_path}\")\n",
                "        except Exception as e:\n",
                "            print(f\"Warning: could not write legacy judge CSV copy: {e}\")\n",
                "    else:\n",
                "        print(\"Nothing pending; judge evaluation already complete.\")\n",
                "\n",
                "    # Convert to DataFrame for Analysis (from persisted CSV)\n",
                "    if judge_csv_path.exists():\n",
                "        llm_judge_df = pd.read_csv(judge_csv_path)\n",
                "        print(\"\\nEvaluation Summary:\")\n",
                "        print(llm_judge_df[['Scenario ID', 'Overall Score', 'Safety']].describe())\n",
                "    else:\n",
                "        print(\"No results generated.\")"
            ]
        },

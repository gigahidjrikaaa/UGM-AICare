                        "RQ1.1 complete. Wrote: d:\\Astaga Ngoding\\Github\\Skripsi\\UGM-AICare\\backend\\research_evaluation\\runs\\2026-01-07T03-57-13.793092_00-00\\rq1_triage_results.csv\n"
                    ]
                },
                {
                    "name": "stderr",
                    "output_type": "stream",
                    "text": [
                        "\n"
                    ]
                }
            ],
            "source": [
                "# Run RQ1.1 Evaluation (per-scenario traceability + retries)\n",
                "from tqdm.auto import tqdm\n",
                "\n",
                "RQ1_TRIAGE_RESULTS_PATH = RUN_DIR / \"rq1_triage_results.csv\"\n",
                "RQ1_TRIAGE_CHECKPOINT_PATH = RUN_DIR / \"rq1_triage_checkpoint.json\"\n",
                "\n",
                "\n",
                "def _load_rq1_triage_checkpoint() -> dict:\n",
                "    if not RQ1_TRIAGE_CHECKPOINT_PATH.exists():\n",
                "        return {}\n",
                "    try:\n",
                "        return json.loads(RQ1_TRIAGE_CHECKPOINT_PATH.read_text(encoding=\"utf-8\"))\n",
                "    except Exception as e:\n",
                "        print(f\"Warning: failed to read checkpoint: {e}\")\n",
                "        return {}\n",
                "\n",
                "\n",
                "def _save_rq1_triage_checkpoint(data: dict) -> None:\n",
                "    try:\n",
                "        write_json(RQ1_TRIAGE_CHECKPOINT_PATH, data)\n",
                "    except Exception as e:\n",
                "        print(f\"Warning: failed to write checkpoint: {e}\")\n",
                "\n",
                "\n",
                "# Resume support: load previous partial results (if any)\n",
                "aika_results = []\n",
                "if RQ1_TRIAGE_RESULTS_PATH.exists():\n",
                "    try:\n",
                "        aika_results = pd.read_csv(RQ1_TRIAGE_RESULTS_PATH).to_dict(orient=\"records\")\n",
                "        print(f\"Loaded partial results: {RQ1_TRIAGE_RESULTS_PATH}\")\n",
                "    except Exception as e:\n",
                "        print(f\"Warning: failed to read existing results CSV: {e}\")\n",
                "        aika_results = []\n",
                "\n",
                "checkpoint = _load_rq1_triage_checkpoint()\n",
                "checkpoint_offset = checkpoint.get(\"next_offset\")\n",
                "\n",
                "start_offset = 0\n",
                "if isinstance(checkpoint_offset, int) and checkpoint_offset >= 0:\n",
                "    start_offset = checkpoint_offset\n",
                "elif isinstance(aika_results, list) and len(aika_results) > 0:\n",
                "    # Best-effort resume: assume results list covers first N scenarios.\n",
                "    start_offset = len(aika_results)\n",
                "\n",
                "# Avoid duplicates if both CSV and checkpoint exist.\n",
                "start_offset = max(start_offset, len(aika_results))\n",
                "\n",
                "print(\"Running RQ1.1: Real-Time Triage Evaluation...\")\n",
                "if start_offset > 0:\n",
                "    print(f\"Resuming from scenario offset {start_offset}/{len(rq1_df)}\")\n",
                "\n",
                "stopped_due_to_quota = False\n",
                "\n",
                "for offset, (_, row) in enumerate(tqdm(rq1_df.iloc[start_offset:].iterrows(), total=len(rq1_df) - start_offset), start=start_offset):\n",
                "    iter_idx = offset + 1\n",
                "    scenario = {\n",
                "        \"id\": row[\"id\"],\n",
                "        \"turns\": row[\"turns\"],\n",
                "        \"is_crisis\": row[\"is_crisis\"],\n",
                "    }\n",
                "\n",
                "    test_id = f\"rq1_triage::{scenario['id']}\"\n",
                "    result = run_with_retry(test_id, lambda: evaluate_aika_triage(scenario))\n",
                "\n",
                "    # Extract text for display (last user message)\n",
                "    user_turns = [t for t in scenario[\"turns\"] if t[\"role\"] == \"user\"]\n",
                "    last_message = user_turns[-1][\"content\"] if user_turns else \"N/A\"\n",
                "\n",
                "    # Normalize access even on error\n",
                "    metadata = result.get(\"metadata\", {}) if isinstance(result, dict) else {}\n",
                "    error = result.get(\"error\") if isinstance(result, dict) else None\n",
                "    error_code = result.get(\"error_code\") if isinstance(result, dict) else None\n",
                "    error_details = result.get(\"error_details\") if isinstance(result, dict) else None\n",
                "    latency = result.get(\"latency\", 0) if isinstance(result, dict) else 0\n",
                "    attempts_used = result.get(\"attempts_used\") if isinstance(result, dict) else None\n",
                "\n",
                "    predicted_risk = metadata.get(\"risk_level\", \"N/A\") if metadata else \"N/A\"\n",
                "    predicted = (predicted_risk in [\"high\", \"critical\"]) if predicted_risk != \"N/A\" else None\n",
                "    ground_truth = scenario[\"is_crisis\"]\n",
                "\n",
                "    # If quota is exhausted, checkpoint and stop so reruns resume here.\n",
                "    if error_code == \"RESOURCE_EXHAUSTED\":\n",
                "        stopped_due_to_quota = True\n",
                "\n",
                "        extra = {}\n",
                "        if isinstance(error_details, dict):\n",
                "            for key in [\"provider\", \"model\", \"api_key_index\", \"api_key_last4\", \"retry_after_s\"]:\n",
                "                if key in error_details and error_details[key] is not None:\n",
                "                    extra[key] = error_details[key]\n",
                "\n",
                "        report_iteration_result(\n",
                "            rq=\"RQ1.1\",\n",
                "            iteration=iter_idx,\n",
                "            total=len(rq1_df),\n",
                "            item_id=str(scenario[\"id\"]),\n",
                "            expected=bool(ground_truth),\n",
                "            predicted=None,\n",
                "            risk=str(predicted_risk),\n",
                "            latency_s=float(latency) if latency is not None else None,\n",
                "            attempts_used=int(attempts_used) if isinstance(attempts_used, int) else None,\n",
                "            error=f\"RESOURCE_EXHAUSTED: {error}\",\n",
                "            extra=extra if extra else None,\n",
                "        )\n",
                "\n",
                "        _save_rq1_triage_checkpoint(\n",
                "            {\n",
                "                \"next_offset\": offset,\n",
                "                \"scenario_id\": scenario[\"id\"],\n",
                "                \"reason\": \"RESOURCE_EXHAUSTED\",\n",
                "                \"details\": extra,\n",
                "                \"note\": \"Rerun the RQ1.1 cell to resume from this scenario.\",\n",
                "            }\n",
                "        )\n",
                "\n",
                "        print(f\"Stopped early due to RESOURCE_EXHAUSTED. Checkpoint: {RQ1_TRIAGE_CHECKPOINT_PATH}\")\n",
                "        break\n",
                "\n",
                "    aika_results.append(\n",
                "        {\n",
                "            \"id\": scenario[\"id\"],\n",
                "            \"input\": last_message,\n",
                "            \"ground_truth\": ground_truth,\n",
                "            \"predicted\": predicted,\n",
                "            \"risk_level_str\": predicted_risk,\n",
                "            \"latency\": latency,\n",
                "            \"attempts_used\": attempts_used,\n",
                "            \"error\": error,\n",
                "            \"error_code\": error_code,\n",
                "            \"is_correct\": (ground_truth == predicted) if predicted is not None else None,\n",
                "        }\n",
                "    )\n",
                "\n",
                "    # Per-iteration visibility (requested): show the outcome for this scenario immediately.\n",
                "    report_iteration_result(\n",
                "        rq=\"RQ1.1\",\n",
                "        iteration=iter_idx,\n",
                "        total=len(rq1_df),\n",
                "        item_id=str(scenario[\"id\"]),\n",
                "        expected=bool(ground_truth),\n",
                "        predicted=predicted,\n",
                "        risk=str(predicted_risk),\n",
                "        latency_s=float(latency) if latency is not None else None,\n",
                "        attempts_used=int(attempts_used) if isinstance(attempts_used, int) else None,\n",
                "        error=str(error) if error else None,\n",
                "    )\n",
                "\n",
                "# Persist per-scenario summary alongside trace log (even for partial runs)\n",
                "pd.DataFrame(aika_results).to_csv(RQ1_TRIAGE_RESULTS_PATH, index=False)\n",
                "print(f\"Wrote: {RQ1_TRIAGE_RESULTS_PATH}\")\n",
                "\n",
                "if not stopped_due_to_quota:\n",
                "    # Clear checkpoint on success (optional, but avoids accidental stale resumes)\n",
                "    if RQ1_TRIAGE_CHECKPOINT_PATH.exists():\n",
                "        try:\n",
                "            RQ1_TRIAGE_CHECKPOINT_PATH.unlink()\n",
                "        except Exception as e:\n",
                "            print(f\"Warning: failed to remove checkpoint: {e}\")\n",
                "\n",
                "    print(\"RQ1.1 complete.\")"
            ]
        },
        {
            "cell_type": "markdown",
            "id": "75ddd264",
            "metadata": {},
            "source": [
                "### 2.3a. Summarize and Visualize RQ1.1 Results (Real-Time Triage)\n",
                "\n",
                "This block converts the raw per-scenario outputs (`aika_results`) into (i) a clean results table used later in the comparative analysis, (ii) standard classification metrics (including the safety-weighted F2-score), and (iii) basic visualizations:\n",
                "\n",
                "- A confusion matrix to inspect false negatives/false positives.\n",
                "- A latency distribution plot to validate the real-time constraint.\n",
                "- A risk-level frequency chart to sanity-check class/risk distribution.\n",
                "\n",
                "**Note:** This step does not store model reasoning. It only aggregates predicted labels, latencies, and metadata fields already emitted by the API."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": 69,
            "id": "153d2d45",
            "metadata": {},
            "outputs": [
                {
                    "name": "stdout",
                    "output_type": "stream",
                    "text": [
                        "RQ1.1 results exist, but no valid predictions were recorded (likely API errors).\n"
                    ]
                }
            ],
            "source": [
                "# --- RQ1.1: Metrics + Visualizations (Aika Real-Time Triage) ---\n",
                "\n",
                "# Convert raw results into a dataframe.\n",
                "aika_results_raw_df = pd.DataFrame(aika_results)\n",
                "\n",
                "if aika_results_raw_df.empty:\n",
                "    print(\"No RQ1.1 results found. Run the RQ1.1 evaluation cell first.\")\n",
                "else:\n",
                "    # Keep only rows with a boolean prediction.\n",
                "    valid_aika_df = aika_results_raw_df.dropna(subset=[\"predicted\"]).copy()\n",
                "    valid_aika_df = valid_aika_df[valid_aika_df[\"predicted\"].isin([True, False])]\n",
                "\n",
                "    if valid_aika_df.empty:\n",
                "        print(\"RQ1.1 results exist, but no valid predictions were recorded (likely API errors).\")\n",
                "    else:\n",
                "        y_true = valid_aika_df[\"ground_truth\"].astype(bool)\n",
                "        y_pred = valid_aika_df[\"predicted\"].astype(bool)\n",
                "\n",
                "        # Classification report (table form for thesis readability)\n",
                "        report = classification_report(\n",
                "            y_true,\n",
                "            y_pred,\n",
                "            target_names=[\"Non-Crisis\", \"Crisis\"],\n",
                "            output_dict=True,\n",
                "            zero_division=0,\n",
                "        )\n",
                "        report_df = pd.DataFrame(report).transpose()\n",
                "        print(\"--- RQ1.1: Aika Real-Time Triage Performance ---\")\n",
                "        display(report_df)\n",
                "\n",
                "        # Key metrics (aligned with RQ1.2 for comparability)\n",
                "        tn, fp, fn, tp = confusion_matrix(y_true, y_pred).ravel()\n",
                "        sensitivity = tp / (tp + fn) if (tp + fn) > 0 else 0.0\n",
                "        specificity = tn / (tn + fp) if (tn + fp) > 0 else 0.0\n",
                "        fnr = fn / (tp + fn) if (tp + fn) > 0 else 0.0\n",
                "        precision = tp / (tp + fp) if (tp + fp) > 0 else 0.0\n",
                "\n",
                "        beta = 2\n",
                "        f2 = (\n",
                "            (1 + beta**2) * (precision * sensitivity) / ((beta**2 * precision) + sensitivity)\n",
                "            if ((beta**2 * precision) + sensitivity) > 0\n",
                "            else 0.0\n",
                "        )\n",
                "\n",
                "        numerator = (tp * tn) - (fp * fn)\n",
                "        denominator = np.sqrt((tp + fp) * (tp + fn) * (tn + fp) * (tn + fn))\n",
                "        mcc = (numerator / denominator) if denominator > 0 else 0.0\n",
                "\n",
                "        p50_latency = float(valid_aika_df[\"latency\"].quantile(0.5))\n",
                "        p95_latency = float(valid_aika_df[\"latency\"].quantile(0.95))\n",
                "\n",
                "        print(\"\\n--- Key Metrics (RQ1.1) ---\")\n",
                "        print(f\"Sensitivity (Recall for Crisis): {sensitivity:.2%}\")\n",
                "        print(f\"Specificity (Recall for Non-Crisis): {specificity:.2%}\")\n",
                "        print(f\"Precision (Positive Predictive Value): {precision:.2%}\")\n",
                "        print(f\"False Negative Rate (FNR): {fnr:.2%}\")\n",
                "        print(f\"F2-Score (Safety Critical): {f2:.4f}\")\n",
                "        print(f\"MCC: {mcc:.4f}\")\n",
                "        print(f\"p50 Latency: {p50_latency:.4f} seconds\")\n",
                "        print(f\"p95 Latency: {p95_latency:.4f} seconds\")\n",
                "\n",
                "        # Create the canonical dataframe used later (Tier 1 vs Tier 2 comparison).\n",
                "        aika_results_df = pd.DataFrame(\n",
                "            {\n",
                "                \"Scenario ID\": valid_aika_df[\"id\"],\n",
                "                \"Expected Crisis\": y_true.values,\n",
                "                \"Predicted Crisis\": y_pred.values,\n",
                "                \"Risk Level\": valid_aika_df.get(\"risk_level_str\"),\n",
                "                \"Latency (s)\": valid_aika_df[\"latency\"].astype(float).values,\n",
                "                \"Error\": valid_aika_df.get(\"error\"),\n",
                "            }\n",
                "        )\n",
                "\n",
                "        print(\"\\n--- RQ1.1 Results Table (for downstream cells) ---\")\n",
                "        display(aika_results_df.head(10))\n",
                "\n",
                "        # Visualizations\n",
                "        fig, (ax1, ax2, ax3) = plt.subplots(1, 3, figsize=(18, 5))\n",
                "\n",
                "        # 1) Confusion matrix\n",
                "        cm = confusion_matrix(y_true, y_pred)\n",
                "        sns.heatmap(\n",
                "            cm,\n",
                "            annot=True,\n",
                "            fmt=\"d\",\n",
                "            cmap=\"Blues\",\n",
                "            ax=ax1,\n",
                "            xticklabels=[\"Non-Crisis\", \"Crisis\"],\n",
                "            yticklabels=[\"Non-Crisis\", \"Crisis\"],\n",
                "        )\n",
                "        ax1.set_title(\"Aika (Tier 1): Confusion Matrix\")\n",
                "        ax1.set_xlabel(\"Predicted\")\n",
                "        ax1.set_ylabel(\"Actual\")\n",
                "\n",
                "        # 2) Latency distribution (real-time constraint)\n",
                "        sns.boxplot(y=valid_aika_df[\"latency\"].astype(float), ax=ax2)\n",
                "        ax2.set_title(\"Aika (Tier 1): Latency Distribution\")\n",
                "        ax2.set_ylabel(\"Latency (seconds)\")\n",
                "\n",
                "        # 3) Risk level frequency (sanity check)\n",
                "        risk_counts = valid_aika_df.get(\"risk_level_str\")\n",
                "        if risk_counts is not None:\n",
                "            risk_counts = risk_counts.fillna(\"N/A\").astype(str).value_counts()\n",
                "            risk_counts.plot(kind=\"bar\", ax=ax3)\n",
                "            ax3.set_title(\"Aika (Tier 1): Predicted Risk Level Frequency\")\n",
                "            ax3.set_xlabel(\"risk_level\")\n",
                "            ax3.set_ylabel(\"count\")\n",
                "        else:\n",
                "            ax3.set_visible(False)\n",
                "\n",
                "        plt.tight_layout()\n",
                "        plt.show()"
            ]
        },
        {
            "cell_type": "markdown",
            "id": "35b41e46",
            "metadata": {},
            "source": [
                "### Interpretation of RQ1.1 Results (Real-Time Triage)\n",
                "\n",
                "The metrics above evaluate the **Real-Time Triage** capability of the Aika agent.\n",
                "\n",
                "*   **Sensitivity (Recall):** Measures the proportion of actual crises correctly identified. A high sensitivity is crucial for safety.\n",
                "*   **Precision:** Measures the proportion of predicted crises that were actual crises. High precision means fewer false alarms.\n",
                "*   **F2-Score:** This is the **primary safety metric**. Unlike the standard F1-Score, the F2-Score weights **Recall** twice as heavily as Precision ($\\beta=2$). In mental health triage, missing a crisis (False Negative) is significantly more dangerous than a false alarm (False Positive). Therefore, a high F2-Score indicates a system that prioritizes user safety.\n",
                "*   **MCC (Matthews Correlation Coefficient):** A balanced metric that takes into account true and false positives and negatives. A value close to +1 represents a perfect prediction, 0 an average random prediction and -1 an inverse prediction.\n",
                "\n",
                "**Inference:**\n",
                "If the **Sensitivity** is lower than 100%, it indicates that the lightweight real-time model (Aika) missed some subtle crisis cues. This justifies the need for the **Tier 2 (STA)** retrospective audit to catch these missed cases. The **Latency** metric confirms whether the system is fast enough for real-time interaction (ideally < 2 seconds)."
            ]
        },
        {
            "cell_type": "markdown",
            "id": "92040fc1",
            "metadata": {},
            "source": [
                "### 2.4. Define RQ1.2 Evaluation Logic (Retrospective Audit)\n",
                "The `evaluate_sta_audit` function tests the Tier 2 safety net. It seeds the full conversation history into the database and then manually triggers the STA to perform a retrospective risk analysis."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": 66,
            "id": "c0c0dcbb",
            "metadata": {},
            "outputs": [
                {
                    "name": "stdout",
                    "output_type": "stream",
                    "text": [
                        "Loaded configuration from: d:\\Astaga Ngoding\\Github\\Skripsi\\UGM-AICare\\backend\\.env\n",
                        "Connecting to DB: ep-proud-cake-a1cvsg9u-pooler.ap-southeast-1.aws.neon.tech/neondb?sslmode=require&channel_binding=require\n",
                        "STA audit evaluation function defined (using .env configuration).\n"
                    ]
                }
            ],
            "source": [
                "import uuid\n",
                "import time\n",
                "import os\n",
                "from pathlib import Path\n",
                "from datetime import datetime\n",
                "from sqlalchemy import create_engine, text\n",
                "from sqlalchemy.orm import sessionmaker\n",
                "from dotenv import load_dotenv\n",
                "\n",
                "# --- RQ1.2: Retrospective Audit (STA) ---\n",
                "# We need direct DB access to seed conversations for the STA analysis endpoint.\n",
                "\n",
                "# 1. Load Configuration from .env\n",
                "try:\n",
                "    # Assuming notebook is in backend/research_evaluation/\n",
                "    # .env is in backend/ (parent) or root (parent.parent)\n",
                "    # The user's workspace structure shows .env in UGM-AICare/ (which is backend root effectively?)\n",
                "    # Let's try to find it.\n",
                "    env_path = Path.cwd().parent / '.env' # Try backend/.env\n",
                "    if not env_path.exists():\n",
                "         env_path = Path.cwd().parent.parent / '.env' # Try root/.env\n",
                "    \n",
                "    if env_path.exists():\n",
                "        load_dotenv(dotenv_path=env_path, override=True)\n",
                "        print(f\"Loaded configuration from: {env_path}\")\n",
                "    else:\n",
                "        print(\"‚ö†Ô∏è .env file not found. Using defaults.\")\n",
                "except Exception as e:\n",
                "    print(f\"Warning loading .env: {e}\")\n",
                "\n",
                "# 2. Configure Database Connection\n",
                "raw_db_url = os.getenv(\"DATABASE_URL\")\n",
                "\n",
                "if not raw_db_url:\n",
                "    print(\"‚ö†Ô∏è DATABASE_URL not set in environment. Defaulting to localhost.\")\n",
                "    TEST_DATABASE_URL = \"postgresql://postgres:postgres@127.0.0.1:5432/aicare_db\"\n",
                "else:\n",
                "    # Adapt the URL for synchronous SQLAlchemy (psycopg2)\n",
                "    # The main app likely uses 'postgresql+asyncpg://...'\n",
                "    TEST_DATABASE_URL = raw_db_url.replace(\"+asyncpg\", \"\")\n",
                "    \n",
                "    # Ensure we use a synchronous driver if not specified\n",
                "    if \"postgresql://\" in TEST_DATABASE_URL and \"+\" not in TEST_DATABASE_URL:\n",
                "        # It's just postgresql://..., sqlalchemy defaults to psycopg2 usually\n",
                "        pass\n",
                "        \n",
                "    # Handle Docker Hostnames for Local Execution\n",
                "    # If the notebook runs on the host (Windows), it cannot resolve 'db' or 'postgres' container names.\n",
                "    # It must connect via localhost mapped port (5432).\n",
                "    if \"@db:\" in TEST_DATABASE_URL:\n",
                "        TEST_DATABASE_URL = TEST_DATABASE_URL.replace(\"@db:\", \"@127.0.0.1:\")\n",
                "    elif \"@postgres:\" in TEST_DATABASE_URL:\n",
                "        TEST_DATABASE_URL = TEST_DATABASE_URL.replace(\"@postgres:\", \"@127.0.0.1:\")\n",
                "        \n",
                "    # Force 127.0.0.1 over localhost to avoid IPv6 issues on Windows\n",
                "    TEST_DATABASE_URL = TEST_DATABASE_URL.replace(\"@localhost:\", \"@127.0.0.1:\")\n",
                "\n",
                "print(f\"Connecting to DB: {TEST_DATABASE_URL.split('@')[-1]}\") # Log safe part\n",
                "\n",
                "# Dispose of any previous engine to clear stale connections\n",
                "try:\n",
                "    engine.dispose()  # type: ignore[name-defined]\n",
                "except NameError:\n",
                "    pass  # engine not defined yet, which is expected on first run\n",
                "\n",
                "# Create Engine with pre-ping to handle dropped connections\n",
                "engine = create_engine(TEST_DATABASE_URL, pool_pre_ping=True, pool_recycle=3600)\n",
                "TestingSessionLocal = sessionmaker(autocommit=False, autoflush=False, bind=engine)\n",
                "\n",
                "STA_CRISIS_THRESHOLD = 2\n",
                "\n",
                "def evaluate_sta_audit(scenario: dict) -> tuple[dict, float]:\n",
                "    \"\"\"\n",
                "    Seeds a conversation into the DB (if not exists) and triggers STA analysis.\n",
                "    \"\"\"\n",
                "    session_id = f\"eval_rq1_{scenario['id']}\"\n",
                "    turns = scenario['turns']\n",
                "    \n",
                "    db = TestingSessionLocal()\n",
                "    try:\n",
                "        # 1. Check if conversation already exists\n",
                "        check_query = text(\"SELECT COUNT(*) FROM conversations WHERE session_id = :session_id\")\n",
                "        result = db.execute(check_query, {\"session_id\": session_id})\n",
                "        count = result.scalar()\n",
                "        \n",
                "        if count is not None and count > 0:\n",
                "            # print(f\"Scenario {scenario['id']} already seeded. Skipping.\")\n",
                "            pass\n",
                "        else:\n",
                "            # 2. Seed Conversation\n",
                "            print(f\"Seeding scenario {scenario['id']}...\")\n",
                "            for i, turn in enumerate(turns):\n",
                "                db.execute(text(\"\"\"\n",
                "                    INSERT INTO conversations (user_id, session_id, conversation_id, message, response, timestamp)\n",
                "                    VALUES (:user_id, :session_id, :conversation_id, :message, :response, :timestamp)\n",
                "                \"\"\"), {\n",
                "                    \"user_id\": EVAL_USER_ID,\n",
                "                    \"session_id\": session_id,\n",
                "                    \"conversation_id\": session_id,\n",
                "                    \"message\": turn['content'] if turn['role'] == 'user' else \"\",\n",
                "                    \"response\": turn['content'] if turn['role'] == 'assistant' else \"\",\n",
                "                    \"timestamp\": datetime.now()\n",
                "                })\n",
                "            db.commit()\n",
                "            \n",
                "    except Exception as e:\n",
                "        print(f\"Error checking/seeding conversation: {e}\")\n",
                "        db.rollback()\n",
                "        return {\"error\": f\"DB Error: {str(e)}\"}, 0.0\n",
                "    finally:\n",
                "        db.close()\n",
                "    \n",
                "    # 3. Trigger Analysis\n",
                "    endpoint = f\"/api/v1/admin/conversation-assessments/{session_id}/trigger\"\n",
                "    payload = {\"force_refresh\": True}\n",
                "    url = f\"{BACKEND_URL}{endpoint}\"\n",
                "    \n",
                "    headers = {\n",
                "        \"Content-Type\": \"application/json\",\n",
                "        \"accept\": \"application/json\"\n",
                "    }\n",
                "    \n",
                "    # Use ADMIN_API_KEY if available, otherwise fall back to API_KEY (which might be user-level)\n",
                "    # The endpoint requires ADMIN privileges.\n",
                "    token_to_use = None\n",
                "    if 'ADMIN_API_KEY' in globals() and ADMIN_API_KEY:\n",
                "        token_to_use = ADMIN_API_KEY\n",
                "    elif 'API_KEY' in globals() and API_KEY:\n",
                "        # Fallback: maybe the main user is admin?\n",
                "        token_to_use = API_KEY\n",
                "        \n",
                "    if token_to_use:\n",
                "        headers[\"Authorization\"] = f\"Bearer {token_to_use}\"\n",
                "    else:\n",
                "        print(\"‚ùå Error: No API Token available for trigger request.\")\n",
                "        return {\"error\": \"Missing Auth Token\"}, 0.0\n",
                "\n",
                "    start_time = time.time()\n",
                "    try:\n",
                "        session = get_robust_session()\n",
                "        # Increased timeout to 300s\n",
                "        response = session.post(url, headers=headers, json=payload, timeout=300)\n",
                "        \n",
                "        if response.status_code == 403:\n",
                "            print(f\"‚ùå 403 Forbidden for {url}\")\n",
                "            # print(f\"Headers used: {headers}\") # Debug only, don't leak full token in logs\n",
                "            print(f\"Response: {response.text}\")\n",
                "            \n",
                "        response.raise_for_status()\n",
                "        response_data = response.json()\n",
                "    except requests.exceptions.RequestException as e:\n",
                "        response_data = {\"error\": str(e)}\n",
                "        if hasattr(e, 'response') and e.response is not None:\n",
                "             response_data['details'] = e.response.text\n",
                "        \n",
                "    end_time = time.time()\n",
                "    return response_data, end_time - start_time\n",
                "\n",
                "print(\"STA audit evaluation function defined (using .env configuration).\")"
            ]
        },
        {
            "cell_type": "markdown",
            "id": "d053db25",
            "metadata": {},
            "source": [
                "### 2.5. Execute RQ1.2 Evaluation\n",
                "We run the retrospective audit for all scenarios. This step verifies if the STA can correctly flag high-risk conversations that might have been missed or borderline in real-time."
            ]
        },
